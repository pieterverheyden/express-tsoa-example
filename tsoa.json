{
    "entryFile": "./src/app.ts",
    "noImplicitAdditionalProperties": "silently-remove-extras",
    "controllerPathGlobs": [
        "src/controllers/*.ts"
    ],
    "spec": {
        "basePath": "/",
        "outputDirectory": "./src/generated",
        "specVersion": 3
    },
    "routes": {
        "basePath": "/",
        "routesDir": "./src/generated"
    },
    "ignore": [
        "**/node_modules/**"
    ]
}